{
  "env": {
    "es2021": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    // "airbnb",
    // "prettier",
    "plugin:prettier/recommended"
  ],

  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "jsx-a11y/aria-role": "off",
    "no-restricted-syntax": "off",
    "no-unused-vars": "error",
    "no-unused-expressions": "error",
    "consistent-return": "off"
  }
}
